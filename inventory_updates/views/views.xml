<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="product_extend_view">
      <field name="name">sale_extend form</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_only_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='barcode']" position="after">
           <field name="ipakn"/>
           <field name="ipakk"/>
           <field name="ypakk"/>
           <field name="pall"/>
           <!-- <field name="group1"/>
           <field name="group2"/>
           <field name="group3"/> -->
           <field name="originr"/>
           <field name="price_per"/>
           <field name="producer"/>
           <field name="prod_type"/>
           <field name="imagelink"/>
           <field name="environment1"/>
           <field name="environment2"/>
           <field name="environment3"/>
           <field name="environment4"/>
           <field name="environment5"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>